    display: flex;
    flex-direction: row;
    position: absolute;
    align-items: center;
    justify-content: space-around;
    left: 0;
    bottom: 0;
}

.tabItem {
    flex:1;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    margin: 5rpx 1rpx;
}